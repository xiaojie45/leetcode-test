{
	"name": "leetcode-test",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@types/jest": "^23.3.10",
		"@types/node": "^10.12.18",
		"@types/react": "^16.7.18",
		"@types/react-dom": "^16.0.11",
		"node-sass": "^4.11.0",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-scripts": "2.1.2",
		"rxjs": "^6.3.3",
		"typescript": "^3.2.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{tsx,js,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@types/enzyme": "^3.1.15",
		"@types/enzyme-adapter-react-16": "^1.0.3",
		"coveralls": "^3.0.2",
		"enzyme": "^3.8.0",
		"enzyme-adapter-react-16": "^1.7.1",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.0",
		"prettier": "1.15.3"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,tsx}",
			"!src/index.{js,jsx,tsx}"
		]
	}
}
